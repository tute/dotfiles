export PATH=$HOME/.bin:$HOME/.rbenv/shims:$HOME/.rbenv/bin:$PATH
eval "$(rbenv init - --no-rehash zsh)"

GPG_TTY=$(tty)
export GPG_TTY

export PATH="./bin:/usr/local/heroku/bin:$PATH"

# Find and replace in project
# Usage: `far search-string replace-string path`
function far() {
  ack -l "$1" | xargs perl -pi -E "s/$1/$2/g"
}

plugins=(brew pass)

# Docker
eval "$(docker-machine env default)"
